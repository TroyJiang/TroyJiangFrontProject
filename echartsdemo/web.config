<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.web>
        <authentication mode="Forms" />
        <identity impersonate="true" />
    </system.web>
    <system.webServer>
        <directoryBrowse enabled="false" />
        <defaultDocument>
            <files>
                <remove value="Default.htm" />
                <add value="Default.html" />
            </files>
        </defaultDocument>
    </system.webServer>
</configuration>
